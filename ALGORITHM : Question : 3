start
input a
input operators ( + , - , * , / , % )
input b 
result = 0
If operator is + then
result = a+b
print "result"
Else if operator is - then
result = a-b
print "result"
Else if operator is x then
result = a*b
print "result"
Else if operator is / then
result = a/b
print "result"
Else if operator is % then
result = a%b
print "result"
Else 
print "you enter wrong operator"
END 
